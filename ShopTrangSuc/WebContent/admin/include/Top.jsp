<%@ page language="java" contentType="text/html; charset=utf-8"
	pageEncoding="utf-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<title>Insert title here</title>
</head>
<body>
	<div id="page-top-outer"
		style="background-color: #356B67;">

		<!-- Start: page-top -->
		<div id="page-top">

			<!-- start logo -->
			<div id="logo">
				<a href="#"> <img src="../images/shared/Logo.png" width="300"
					height="70" alt="" /></a>
			</div>
			<!-- end logo -->

			<!--  start top-search -->
			<div id="top-search">
				<table border="0">
					<tr>
						<td><input type="text" value="Search"
							onblur="if (this.value=='') { this.value='Search'; }"
							onfocus="if (this.value=='Search') { this.value=''; }"
							class="top-search-inp"
							style="background: url(../images/shared/top_search_inp.gif) no-repeat;" /></td>
						<td><select class="styledselect">
								<option value="">Tất cả</option>
								<option value="">Đặt vé</option>
								<option value="">Đăng ký</option>
								<option value="">Thành viên</option>
								<option value="">Tin tức</option>
						</select></td>
						<td><input type="image"
							src="../images/shared/top_search_btn.gif" /></td>
					</tr>
				</table>
			</div>
			<!--  end top-search -->
		</div>
		<!-- End: page-top -->

	</div>
</body>
</html>